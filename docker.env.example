# Copy this file to .env and fill in your actual values

# Required API Keys
GROQ_API_KEY=your_groq_api_key_here
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=gcp-starter

# Optional API Keys (depending on embedding provider)
OPENAI_API_KEY=your_openai_api_key_here

# Configuration Options
EMBEDDING_PROVIDER=huggingface
# Options: openai, huggingface, simple

EMBEDDING_MODEL=Xenova/all-MiniLM-L6-v2
# For OpenAI: text-embedding-ada-002
# For HuggingFace: Xenova/all-MiniLM-L6-v2

GROQ_MODEL=llama-3.3-70b-versatile
# Options: llama-3.3-70b-versatile, mixtral-8x7b-32768, gemma2-9b-it

PINECONE_INDEX_NAME=log-interpreter-index
# Base name for Pinecone index (provider suffix will be added automatically)

# Application Settings
NODE_ENV=production
PORT=3000
