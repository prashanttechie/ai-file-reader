{
  "name": "log-interpreter-agent",
  "version": "1.0.0",
  "description": "A simple LangChain agent with web UI that reads answers from files using Groq + Pinecone",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "src/agent.js",
  "scripts": {
    "start": "node src/agent.js",
    "dev": "node --watch src/agent.js",
    "ui": "node src/server.js",
    "ui-dev": "node --watch src/server.js",
    "example": "node src/example.js",
    "demo": "node src/example.js --demo",
    "groq-demo": "node src/groq-demo.js",
    "free-demo": "node src/free-demo.js",
    "install-deps": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t log-interpreter-agent .",
    "docker:run": "docker run -p 3000:3000 --env-file .env log-interpreter-agent",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:debian": "docker-compose -f docker-compose.alternative.yml --profile debian up -d",
    "docker:ubuntu": "docker build -f Dockerfile.ubuntu -t log-interpreter-agent-ubuntu . && docker run -p 3000:3000 --env-file .env log-interpreter-agent-ubuntu",
    "docker:openai": "docker-compose -f docker-compose.alternative.yml --profile openai up -d",
    "docker:status": "docker ps && echo && docker-compose ps",
    "docker:build-ubuntu": "docker build -f Dockerfile.ubuntu -t log-interpreter-agent-ubuntu .",
    "docker:login": "docker login",
    "docker:tag": "docker tag log-interpreter-agent $DOCKER_USERNAME/log-interpreter-agent:latest",
    "docker:tag-version": "docker tag log-interpreter-agent $DOCKER_USERNAME/log-interpreter-agent:v$npm_package_version",
    "docker:push": "docker push $DOCKER_USERNAME/log-interpreter-agent:latest",
    "docker:push-version": "docker push $DOCKER_USERNAME/log-interpreter-agent:v$npm_package_version",
    "docker:publish": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "docker:publish-version": "npm run docker:build && npm run docker:tag-version && npm run docker:push-version",
    "docker:buildx": "docker buildx build --platform linux/amd64,linux/arm64 -t $DOCKER_USERNAME/log-interpreter-agent:latest --push ."
  },
  "dependencies": {
    "@langchain/community": "^0.2.0",
    "@langchain/groq": "^0.0.14",
    "@langchain/openai": "^0.2.0",
    "@langchain/pinecone": "^0.0.8",
    "@pinecone-database/pinecone": "^3.0.0",
    "@xenova/transformers": "^2.17.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "langchain": "^0.2.0",
    "mammoth": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pdfjs-dist": "^5.4.149"
  }
}
